{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "Build emulator",
			"command": "/usr/bin/gcc",
			"args": [
				"-g",
				"main.c",
				"emulator.c",
				"-o",
				"${fileDirname}/emulator"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "compiler: /usr/bin/gcc"
		}
	]
}